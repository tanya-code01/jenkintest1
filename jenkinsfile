pipeline {
    agent any 
    tools{
        nodejs 'NodeJS 23.10.0'
    }
    stages {
        stage 'Install'{
            steps{
            sh 'npm install'
            }
        }
        stage 'Build'{
            steps{
                sh 'npm run build'
            }
        }
        stage 'Test'{
            steps{
               sh 'npm test || echo "No tests defined"'
        }
        }
    }
}
